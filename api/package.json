{
  "name": "@techzu-pos/api",
  "version": "1.0.0",
  "private": true,
  "description": "Express.js REST API for TechzuPOS system",
  "main": "./bin/www",
  "scripts": {
    "dev": "nodemon ./bin/www",
    "start": "node ./bin/www",
    "build": "prisma generate",
    "postinstall": "prisma generate",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf node_modules",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.0",
    "dotenv": "^16.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "prisma": "^5.6.0"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
