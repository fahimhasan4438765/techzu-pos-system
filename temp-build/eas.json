{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.9.0",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.9.0",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "production"
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.9.0",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
